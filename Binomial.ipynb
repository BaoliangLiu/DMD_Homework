   "source": [
    "import pylab as pl\n",
    "%matplotlib inline\n",
    "import numpy as np\n",
    "from scipy import stats\n",
    "\n",
    "n=6 # 个数\n",
    "k=np.arange(n+1)\n",
    "binomial=stats.binom.pmf(k,n,0.3)  # 0.3为概率\n",
    "\n",
    "pl.stem(k,binomial,basefmt=\"k-\")\n",
    "pl.margins(0.1)\n",
    "\n",
    "print binomial"
   ]
